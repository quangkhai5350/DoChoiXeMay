    <section id="navigation" class="dark-nav">
        <!-- Navigation Inner -->
        <div class="nav-inner">
            <!-- Site Logo -->
            <div class="site-logo">
                <a href="#" id="logo-text" class="scroll logo">
                    <img src="~/Areas/Admin/Content/img/logo/Teklogoo.png" alt="" style="width:50px;margin-bottom:10px" />
                    @if (Session["UserName"] != null)
                    {
                        <span>@Session["UserName"].ToString()</span>
                    }
                    else
                    {
                        <span>Trần Nguyễn TechZone</span>
                    }
                </a>
            </div><!-- End Site Logo -->
            <a class="mini-nav-button gray2"><i class="fa fa-bars"></i></a>
            <!-- Navigation Menu -->
            <div class="nav-menu">
                <ul class="nav uppercase">
                    <li style="display:inline-block !important"><a href="/Home/Index" class="scroll">Home</a></li>
                    <li style="display:inline-block !important"><a href="/Active/Index" class="scroll">Warranty</a></li>
                    @if (Session["UserName"] != null)
                    {
                        <li style="display:inline-block !important"><a href="/Home/LogOut" class="scroll">LogOut</a></li>
                    }
                    else
                    {
                        <li style="display:inline-block !important"><a href="/LoginWeb" class="scroll">LoginWebTEK</a></li>
                    }
                </ul>
            </div><!-- End Navigation Menu -->
        </div><!-- End Navigation Inner -->
    </section>
    <!-- End Navigation Section -->
    

    <div class="container body-content" style="min-height:750px">
        @RenderBody()
        <hr />
    </div>
    <!-- Footer -->
    <footer id="footer" class="footer">
        <div class="container">
            <div style="margin-top:10px;padding-top:15px;padding-bottom:30px;color:white">
                <div class="row text-center">
                    <h5>
                        MST: 3703251330
                    </h5>
                </div>
                <div class="row text-center">
                    <h5>
                        CÔNG TY TNHH THƯƠNG MẠI-DỊCH VỤ-SẢN XUẤT TRẦN NGUYỄN TECHZONE
                    </h5>
                </div>
                <div class="row text-center">
                    <h5>
                        311 Đường Phạm Ngọc Thạch, Tổ 88, Khu Phố 6, Phường Hiệp Thành, Thành Phố Thủ Dầu Một, Tỉnh Bình Dương.
                    </h5>
                </div>
            </div>
        </div>
    </footer><!-- End Footer -->
